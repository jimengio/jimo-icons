{
  "name": "@jimengio/jimo-icons",
  "version": "0.1.26-a1",
  "description": "Self-made icons",
  "main": "lib/jimo-icon.js",
  "types": "./lib/jimo-icon.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen": "coffee scripts/convert.coffee",
    "compile": "tsc -d --project tsconfig-compile.json --outDir lib/",
    "cp": "rm -rf lib/* && cp -r src/fonts lib/",
    "watch": "webpack-dev-server",
    "build": "webpack --config=webpack.release.js",
    "release": "yarn build",
    "upload": "rsync -avr --progress dist/* fe.jimu.io:repo/jimengio/jimo-icons",
    "postinstall": "rm -rfv node_modules/@types/react-*/node_modules/@types/react",
    "prepare": "yarn gen && yarn cp && yarn compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimengio/jimo-icons.git"
  },
  "keywords": [
    "icon"
  ],
  "author": "JIMENG.IO",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/jimengio/jimo-icons/issues"
  },
  "homepage": "https://github.com/jimengio/jimo-icons#readme",
  "devDependencies": {
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "awesome-typescript-loader": "^5.2.1",
    "coffeescript": "^2.4.1",
    "css-loader": "^2.1.1",
    "emotion": "^10.0.9",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "svgo": "^1.2.1",
    "typescript": "^3.4.2",
    "url-loader": "^1.1.2",
    "webfonts-generator": "sunflowerdeath/webfonts-generator",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.0"
  }
}
